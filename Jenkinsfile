pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy'
            }  
        }
        stage('test') {
            steps {
                echo 'testing'
            }   
        }
        stage('release') {
            steps {
                echo 'Releasing'
            }  
        }
    }
}
